<?xml version="1.0" encoding="utf-8"?>
<com.koolearn.android.kooreader.popup.MainMenuWindow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:id="@+id/mainmenuwindow"
    android:background="@color/window_background"
    >
    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:background="@color/white">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp"
            android:id="@+id/back"
            android:layout_centerVertical="true"
            android:background="@mipmap/yuedu_fanhui"/>
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mark"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/shuqian"/>
    </RelativeLayout>


</com.koolearn.android.kooreader.popup.MainMenuWindow>